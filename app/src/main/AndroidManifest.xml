<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caowj.newtask">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 在SD卡中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_FORMAT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name=".base.BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:configChanges="orientation|screenSize|uiMode"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name=".SplashActivity2"
            android:enabled="false"
            android:icon="@drawable/home"
            android:label="工程2"
            android:targetActivity=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>
        <activity-alias
            android:name=".SplashActivity3"
            android:enabled="false"
            android:icon="@drawable/location"
            android:label="工程3"
            android:targetActivity=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|uiMode"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".example.activity.FunctionListActivity"
            android:label="功能列表">
            <intent-filter>
                <action android:name="${applicationId}.FunctionList" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".example.activity.BottomNavigationActivity"
            android:label="导航栏" />
        <activity
            android:name=".module1.activity.MvpLoginActivity"
            android:label="MVP模式的登录" />
        <activity android:name=".example.activity.TranslucentSystemBarActivity" />
        <activity android:name=".example.activity.SwipeRefreshActivity" />
        <activity
            android:name=".example.activity.ElemSearchActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".example.activity.ElemSearchResultActivity"
            android:theme="@style/SearchTheme" />
        <activity android:name=".example.activity.IndexableListActivity" />
        <activity android:name=".example.activity.IndexableListActivity2" />
        <activity android:name=".example.activity.ViewShowActivity" />
        <activity android:name=".example.activity.ZoomImageActivity" />
        <activity android:name=".example.activity.GestureDetectorActivity" />
        <activity android:name=".example.activity.SearchViewActivity" />
        <activity android:name=".example.activity.TestCommonAdapterActivity1" />
        <activity android:name=".example.activity.TestCommonAdapterActivity2" />
        <activity android:name=".example.activity.TestMinaActivity" />

        <service android:name=".example.mina.MinaService" />

        <activity android:name=".example.activity.TestDaggerActivity" />
        <activity android:name=".example.activity.TestDaggerActivity2" />
        <activity android:name=".example.activity.TestDaggerActivity3" />
        <activity android:name=".example.activity.TextViewShowHtmlActivity" />
        <activity android:name=".example.activity.TestRrmActivity" />
        <activity android:name=".module1.activity.QipaiNewActivity" />
        <activity
            android:name=".example.activity.ElemTabActivity"
            android:theme="@style/ElemTheme" />
        <activity android:name=".example.activity.TestGreenDaoActivity" />
        <activity android:name=".example.activity.TestLruCacheActivity" />
        <activity android:name=".example.activity.TestLruCacheActivity2" />
        <activity android:name=".example.activity.TextSwitcherActivity" />
        <activity android:name=".example.activity.TestThreadPoolActivity" />
        <activity android:name=".example.activity.TestAudioActivity" />
        <activity android:name=".example.activity.TestAudioListActivity" />
        <activity android:name=".example.activity.TestTextureViewActivity" />

        <receiver android:name=".other.BootupReceiver">
            <intent-filter android:priority="1000">
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
            <intent-filter android:priority="1000">
                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.MEDIA_EJECT" />

                <data android:scheme="file" />
            </intent-filter>
        </receiver>

        <activity android:name=".example.activity.TestIntentServiceActivity" />

        <service android:name=".example.service.MyIntentService" />

        <activity android:name=".example.activity.TestFragmentDialogActivity" />
        <activity android:name=".example.activity.TestUIActivity" />
        <activity
            android:name=".example.activity.TestNestedScrollActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".module1.activity.QipaiIndexActivity" />
        <activity android:name=".example.activity.TestCountdownActivity" />
        <activity android:name=".example.activity.TestOpenGLActivity" />
        <activity android:name=".example.activity.TestSynchronizedActivity" />
        <activity android:name=".example.activity.TestWeiboDetailActivity" />
        <activity android:name=".example.activity.TestVelocityTrackerActivity" />
        <activity android:name=".example.activity.TestMasterVoiceActivity" />
        <activity android:name=".example.activity.TestTaoBaoDetailActivity" />
        <activity android:name=".example.activity.TestOkHttpActivity" />
        <activity
            android:name=".toutiao.module.news.channel.NewsChannelActivity"
            android:configChanges="orientation|screenSize|uiMode"
            android:label="@string/title_item_drag"
            android:theme="@style/AppTheme.NoActionBar.Slidable" />
        <activity android:name=".example.activity.TestAnnotationActivity" />
        <activity android:name=".example.activity.TestReflectActivity" />
    </application>

</manifest>